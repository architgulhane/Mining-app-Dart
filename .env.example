# CogniSarthi Cross-Platform App Environment Variables

# Backend API Configuration
API_BASE_URL=http://localhost:8000
API_TIMEOUT=30000

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key-here

# App Configuration
APP_NAME=CogniSarthi
APP_VERSION=1.0.0
ENVIRONMENT=development

# Feature Flags
ENABLE_VOICE_INPUT=true
ENABLE_RAG=true
ENABLE_ANALYTICS=true

# Debug Settings
DEBUG_MODE=true
LOG_LEVEL=info
